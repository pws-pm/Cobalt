                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Color Palette -->
    <Color x:Key="color-palette-fds-c-palette-base">#2E2F33</Color>
