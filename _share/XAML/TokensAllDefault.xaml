    <Color x:Key="color-semantic-fds-c-negative-surface-low-hover">#DE5B5D4D</Color>
    <Color x:Key="color-semantic-fds-c-negative-surface-pressed">#FF9A97</Color>
    <Color x:Key="color-semantic-fds-c-negative-texticon-onnegative">#111111</Color>
    <Color x:Key="color-semantic-fds-c-negative-texticon-onneutral">#FF746C</Color>
    <Color x:Key="color-semantic-fds-c-neutral-background-high">#242529</Color>
    <Color x:Key="color-semantic-fds-c-neutral-background-low">#161719</Color>
    <Color x:Key="color-semantic-fds-c-neutral-background-medium">#1B1D21</Color>
    <Color x:Key="color-semantic-fds-c-neutral-background-minimum">#131314</Color>
    <Color x:Key="color-semantic-fds-c-neutral-background-modaloverlay">#00000099</Color>
