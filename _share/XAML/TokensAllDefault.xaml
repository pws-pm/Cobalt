    <Color x:Key="color-semantic-fds-c-primary-texticon-onprimary">#111111</Color>
    <Color x:Key="color-semantic-fds-c-warning-surface">#F3BE62</Color>
    <Color x:Key="color-semantic-fds-c-warning-surface-hover">#FFD694</Color>
    <Color x:Key="color-semantic-fds-c-warning-texticon-onneutral">#F3BE62</Color>
    <Color x:Key="color-semantic-fds-c-warning-texticon-onwarning">#111111</Color>

    <!-- Effects (Shadows) -->
    <sys:String x:Key="effects-shadow-100">0 1px 4px 0 #0000000A, 0 -0.5px 0.25px -0.25px #FFFFFF33</sys:String>
    <sys:String x:Key="effects-shadow-200">0 3px 8px -2px #0000000D, 0 -0.5px 0.25px -0.25px #FFFFFF33</sys:String>
    <sys:String x:Key="effects-shadow-300">0 4px 12px -4px #0000001A, 0 -1px 0.5px -0.5px #FFFFFF33</sys:String>
    <sys:String x:Key="effects-shadow-400">0 8px 16px -6px #0000001A, 0 -1px 0.5px -0.5px #FFFFFF33</sys:String>
    <sys:String x:Key="effects-shadow-500">0 12px 24px -8px #0000001F, 0 -1px 0.5px -0.5px #FFFFFF33</sys:String>
    <sys:String x:Key="effects-shadow-600">0 20px 24px -10px #00000026, 0 -1px 0.5px -0.5px #FFFFFF33</sys:String>
    <sys:String x:Key="effects-shadow-inset">inset 0 1px 2px 0 #00000012, inset 0 -0.5px 1px 0 #FFFFFF40</sys:String>

    <!-- Radii -->
    <sys:Double x:Key="radius-fds-r-2xl">20</sys:Double>
    <sys:Double x:Key="radius-fds-r-3xl">32</sys:Double>
    <sys:Double x:Key="radius-fds-r-3xl2more">34</sys:Double>
    <sys:Double x:Key="radius-fds-r-lg">12</sys:Double>
    <sys:Double x:Key="radius-fds-r-max">1000</sys:Double>
    <sys:Double x:Key="radius-fds-r-md">8</sys:Double>
    <sys:Double x:Key="radius-fds-r-md2less">6</sys:Double>
    <sys:Double x:Key="radius-fds-r-md2more">10</sys:Double>
    <sys:Double x:Key="radius-fds-r-md4more">12</sys:Double>
    <sys:Double x:Key="radius-fds-r-none">0</sys:Double>
    <sys:Double x:Key="radius-fds-r-s">4</sys:Double>
    <sys:Double x:Key="radius-fds-r-xl">16</sys:Double>
    <sys:Double x:Key="radius-fds-r-xs">2</sys:Double>

    <!-- Spacings -->
    <sys:Double x:Key="spacing-fds-s-2xl">32</sys:Double>
    <sys:Double x:Key="spacing-fds-s-2xs">4</sys:Double>
    <sys:Double x:Key="spacing-fds-s-3xl">48</sys:Double>
    <sys:Double x:Key="spacing-fds-s-3xs">2</sys:Double>
    <sys:Double x:Key="spacing-fds-s-4xl">64</sys:Double>
    <sys:Double x:Key="spacing-fds-s-fixed2">2</sys:Double>
    <sys:Double x:Key="spacing-fds-s-lg">20</sys:Double>
    <sys:Double x:Key="spacing-fds-s-md">16</sys:Double>
    <sys:Double x:Key="spacing-fds-s-none">0</sys:Double>
    <sys:Double x:Key="spacing-fds-s-sm">12</sys:Double>
    <sys:Double x:Key="spacing-fds-s-xl">24</sys:Double>
    <sys:Double x:Key="spacing-fds-s-xs">8</sys:Double>

    <!-- Widths -->
    <sys:Double x:Key="width-fds-w-lg">2</sys:Double>
    <sys:Double x:Key="width-fds-w-md">1</sys:Double>
    <sys:Double x:Key="width-fds-w-sm">0.5</sys:Double>
    <sys:Double x:Key="width-fds-w-xl">4</sys:Double>

</ResourceDictionary>
