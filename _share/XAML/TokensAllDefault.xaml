    <Color x:Key="color-semantic-fds-c-neutral-surface-high-disabled">#0000001A</Color>
    <Color x:Key="color-semantic-fds-c-neutral-surface-high-hover">#3D3E42</Color>
    <Color x:Key="color-semantic-fds-c-neutral-surface-inverted">#FFFFFF</Color>
    <Color x:Key="color-semantic-fds-c-neutral-surface-lighten">#FFFFFF0A</Color>
    <Color x:Key="color-semantic-fds-c-neutral-surface-lighten-hover">#FFFFFF1A</Color>
    <Color x:Key="color-semantic-fds-c-neutral-surface-none-hover">#FFFFFF0D</Color>
    <Color x:Key="color-semantic-fds-c-neutral-surface-none-pressed">#FFFFFF1A</Color>
    <Color x:Key="color-semantic-fds-c-neutral-surface-shift-disabled">#FFFFFF0A</Color>
    <Color x:Key="color-semantic-fds-c-neutral-surface-shift-low">#FFFFFF0D</Color>
    <Color x:Key="color-semantic-fds-c-neutral-surface-shift-low-hover">#FFFFFF1A</Color>
    <Color x:Key="color-semantic-fds-c-neutral-surface-shift-low-pressed">#FFFFFF26</Color>
    <Color x:Key="color-semantic-fds-c-neutral-surface-shift-medium">#FFFFFF26</Color>
    <Color x:Key="color-semantic-fds-c-neutral-surface-shift-medium-hover">#FFFFFF33</Color>
    <Color x:Key="color-semantic-fds-c-neutral-texticon-onneutral-1spot">#FFFFFF6E</Color>
    <Color x:Key="color-semantic-fds-c-neutral-texticon-onneutral-2headline">#FFFFFF8F</Color>
    <Color x:Key="color-semantic-fds-c-neutral-texticon-onneutral-3content">#FFFFFFAD</Color>
    <Color x:Key="color-semantic-fds-c-neutral-texticon-onneutral-4body">#FFFFFFCC</Color>
    <Color x:Key="color-semantic-fds-c-neutral-texticon-onneutral-5fluent">#FFFFFFE6</Color>
    <Color x:Key="color-semantic-fds-c-neutral-texticon-onneutral-inverted">#111111</Color>
    <Color x:Key="color-semantic-fds-c-positive-surface">#379D63</Color>
    <Color x:Key="color-semantic-fds-c-positive-surface-hover">#54B87B</Color>
    <Color x:Key="color-semantic-fds-c-positive-surface-low">#379D6326</Color>
    <Color x:Key="color-semantic-fds-c-positive-surface-low-hover">#379D634D</Color>
    <Color x:Key="color-semantic-fds-c-positive-texticon-onneutral">#54B87B</Color>
    <Color x:Key="color-semantic-fds-c-positive-texticon-onpositive">#111111</Color>
    <Color x:Key="color-semantic-fds-c-primary-stroke">#2384CD</Color>
    <Color x:Key="color-semantic-fds-c-primary-stroke-focus">#2384CD4D</Color>
    <Color x:Key="color-semantic-fds-c-primary-stroke-hover">#4198E3</Color>
    <Color x:Key="color-semantic-fds-c-primary-surface">#2384CD</Color>
    <Color x:Key="color-semantic-fds-c-primary-surface-disabled">#2384CD4D</Color>
